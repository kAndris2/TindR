rid', id);\r\n  }\r\n  removeCookie(){\r\n    Cookies.remove('userid');\r\n  }\r\n  setUser(user) {\r\n    this.setCookie(user.id);\r\n    this.setState({\r\n      user : user,\r\n      isLoggedIn : true,\r\n      isLoading: false\r\n    });\r\n    this.saveCoordinates(user.id);\r\n  }\r\n\r\n  saveCoordinates(id) {\r\n    navigator.geolocation.getCurrentPosition(response => {\r\n      axios.post(`${process.env.REACT_APP_IP}/api/save_position/${id}`,{\r\n        latitude: response.coords.latitude,\r\n        longitude: response.coords.longitude\r\n    })\r\n    .then(() => {\r\n      this.setState({\r\n        isLoading: false\r\n      });\r\n    });\r\n    })\r\n  }\r\n\r\n  async checkLoginStatus() {\r\n    const id = Cookies.get('userid');\r\n    if (id !== undefined) {\r\n      this.setState({isLoading : true})\r\n      await axios.get(process.env.REACT_APP_IP+\"/api/user/\" + id)\r\n      .then(response => {\r\n        this.setState({\r\n          user : response.data,\r\n          isLoggedIn : true,\r\n          isLoading : false\r\n        })\r\n      })\r\n    }\r\n  }\r\n\r\n  async forceRender() {\r\n    this.setState({\r\n      isLoading : true\r\n    });\r\n    await this.componentDidMount();\r\n  }\r\n\r\n  async componentDidMount() {\r\n    await this.checkLoginStatus();\r\n  }\r\n\r\n  render() {\r\n    const {user, isLoggedIn, isLoading, doForce} = this.state;\r\n\r\n    if (!isLoading) {\r\n      return (\r\n        <>\r\n          {isLoggedIn === true &&\r\n            <>\r\n              <SideBar \r\n                removeCookie={this.removeCookie} \r\n                user={this.state.user} \r\n                pageWrapId={'page-wrap'} \r\n                outerContainerId={'outer-container'} \r\n                forceRender={this.forceRender}\r\n              />\r\n\r\n              <Router>\r\n                <Switch>\r\n\r\n                  <Route exact path=\"/tickets/:id\">\r\n                    <CreateTickets \r\n                      userID=