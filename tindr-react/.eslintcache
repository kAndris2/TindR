r\n      const trigger = velocity > 0.2;\r\n\r\n      const dir = xDir < 0 ? -1 : 1;\r\n\r\n      if (!down && trigger) {\r\n        const temp = gone;\r\n        temp.add(index);\r\n        setGone(temp);\r\n        \r\n        const send = {\r\n          \"index\": index,\r\n            \"giverid\": userID.userID,\r\n            \"direction\": dir\r\n        }\r\n\r\n        fetch(`https://${process.env.REACT_APP_IP}:8443/api/give_vote`, {\r\n          method: 'post',\r\n          headers: {'Content-Type':'application/json'},\r\n          body: JSON.stringify(send)\r\n          });\r\n      }\r\n\r\n      set(i => {\r\n        if (index !== i) return;\r\n        const isGone = gone.has(index);\r\n\r\n        const x = isGone ? (200 + window.innerWidth) * dir : down ? xDelta : 0;\r\n\r\n        const rot = xDelta / 100 + (isGone ? dir * 10 * velocity : 0);\r\n\r\n        const scale = down ? 1.1 : 1;\r\n        return {\r\n          x,\r\n          rot,\r\n          scale,\r\n          delay: undefined,\r\n          config: { friction: 50, tension: down ? 800 : isGone ? 200 : 500 }\r\n        };\r\n      });\r\n\r\n      if (!down && gone.size === data.length) {\r\n        console.log('yeyp')\r\n        setTimeout(() => gone.clear() || set(i => to(i)), 600);\r\n      }\r\n    }\r\n  );\r\n\r\n  console.log('gone: ' + gone.size);\r\n  if(cardState.length != gone.size) {\r\n    return (\r\n      <>\r\n      <div id='recommendations'>\r\n        {cardState.map(({ x, y, rot, scale }, i) => (\r\n          <Card\r\n            key={i}\r\n            i={i}\r\n            x={x}\r\n            y={y}\r\n            rot={rot}\r\n            scale={scale}\r\n            trans={trans}\r\n            data={data}\r\n            bind={bind}\r\n          />\r\n        ))}\r\n        <h2>{gone}</h2>\r\n      </div>\r\n      </>\r\n    );\r\n  }\r\n  else {\r\n    return(<Pulse userID={userID.userID} />);\r\n  }\r\n}\r\n\r\nexport default Deck;","D:\\Gyakran hasznalt\\Projects\\Codec